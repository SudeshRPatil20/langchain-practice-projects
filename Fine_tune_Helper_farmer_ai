   "source": [
    "import json\n",
    "\n",
    "data = []\n",
    "with open('soil_qa.jsonl') as file:\n",
    "    for line in file:\n",
    "        features = json.loads(line)\n",
    "\n",
    "        # remove index column if present\n",
    "        features.pop(\"Unnamed: 0\", None)\n",
    "\n",
    "        # build template manually since keys have dots in them\n",
    "        template = (\n",
    "            f\"ANSWER:\\n{features['ANSWER']}\\n\\n\"\n",
    "            f\"QUESTION.question:\\n{features['QUESTION.question']}\\n\\n\"\n",
    "            f\"QUESTION.paragraph:\\n{features['QUESTION.paragraph']}\"\n",
    "        )\n",
    "\n",
    "        data.append(template)\n",
    "\n",
    "# take first 1000 examples\n",
    "data = data[:1000]\n"
   ]
  },
